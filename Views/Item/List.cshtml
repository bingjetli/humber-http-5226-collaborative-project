@model IEnumerable<humber_http_5226_collaborative_project.Models.ItemDto>

@{
    ViewBag.Title = "List";
}

<h2>Browse Items</h2>
<a href="/Item/New" class="new-item-link">New Item</a>

@foreach (var item in Model)
{
    <div class="item-item">
        <h2 class="item-name"><a href="/Item/Details/@item.ItemId">@item.ItemName</a></h2>
    </div>
}

