@model IEnumerable<humber_http_5226_collaborative_project.Models.ItemDto>

@{
    ViewBag.Title = "List";
}

<h2>Browse Items</h2>
<a href="/Item/New">New Item</a>

@foreach (var item in Model)
{
    <div>
        <h3><a href="/Item/Details/@item.ItemId">@item.Name</a></h3>
        <p>@item.Price</p>
    </div>
}

